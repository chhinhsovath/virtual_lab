{
  "name": "virtual-lab-lms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:setup:local": "./scripts/setup-database.sh local",
    "db:setup:production": "./scripts/setup-database.sh production",
    "db:migrate:local": "NODE_ENV=development npm run db:setup:local",
    "db:migrate:production": "NODE_ENV=production npm run db:setup:production",
    "db:reset:local": "PGPASSWORD=12345 psql -h localhost -U postgres -d postgres -c 'DROP DATABASE IF EXISTS virtual_lab; CREATE DATABASE virtual_lab;' && PGPASSWORD=12345 psql -h localhost -U postgres -d virtual_lab -f ./scripts/complete-setup.sql",
    "db:verify": "./scripts/verify-setup.sh",
    "db:test": "./scripts/test-db-connection.sh",
    "type-check": "tsc --noEmit",
    "mcp:build": "tsc src/lib/mcp-server.ts --outDir dist --module esnext --target es2020 --moduleResolution node --esModuleInterop",
    "mcp:start": "node scripts/mcp-server.js",
    "mcp:dev": "npm run mcp:build && npm run mcp:start",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:super-admin": "playwright test tests/super-admin",
    "test:super-admin:headed": "playwright test tests/super-admin --headed",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^5.1.1",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.10.9",
    "@upstash/vector": "^1.1.5",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.294.0",
    "next": "^14.2.30",
    "next-themes": "^0.2.1",
    "openai": "^4.0.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.49.2",
    "recharts": "^2.12.0",
    "sonner": "^1.2.4",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@types/node": "^20.19.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4"
  }
}
